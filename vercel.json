{
    "version": 2,
    "builds": [
      {
        "src": "index.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "webpack.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ],
    "functions": {
      "api/cron.js": {
        "schedule": "0 0 * * *"
      }
    }
  }
  